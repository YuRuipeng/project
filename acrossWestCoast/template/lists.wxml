<template name="lists">
  <block wx:for="{{lists}}" wx:for-index="idx" wx:if="{{lists[idx] !== ''}}" wx:key="{{idx}}">
    <view class='interacts-comment'>
      <view class='headImg'>
        <image src='{{lists[idx].user.avatar32}}'></image>
      </view>
      <view class='comment'>
        <view class='userName'>{{item.user.nickname}}
          <text bindtap='doDelWeibo' data-id='{{item.id}}' wx:if="{{item.can_delete}}">删除</text>
          <text bindtap='doDelWeibo' wx:else></text>
        </view>
        <view class='contant'>
          <text class='contant-text' wx:if="{{item.topics == ''}}"></text>
          <text class='contant-text' style='color:#00b0ec' bindtap='toTopic' data-id="{{item.topics[0].id}}" wx:else>#{{item.topics[0].name}}#</text> {{item.content}}
        </view>
        <view class='images' wx:if="{{item.images == undefind}}"></view>
        <view class='images' wx:else>
          <block wx:for="{{lists[idx].images}}" wx:for-index="o" wx:key="{{o}}">
            <image src='{{item.small}}' mode='aspectFit' bindtap='previewImage' data-id='{{lists[idx].id}}' data-src="{{item.big}}"></image>
          </block>
        </view>
        <view class='result'>
          <view class='result-time'>{{item.create_time}}</view>
          <view class='result-zan result-zan1'>
            <view bindtap="clickzan" style='float: left' data-id="{{item.id}}" id="{{idx}}">
              <block wx:if="{{item.is_support}}">
                <image src="data:image/png;base64,"></image>
              </block>
              <block wx:else>
                <image src="data:image/png;base64,"></image>
              </block>
            </view>
            <image src='data:image/png;base64,'
              catchtap='toCommit' data-id="{{item.id}}"></image>
          </view>
        </view>
      </view>
      <view class='zandata'>
        <view class='zandata-zan' wx:if="{{item.supported.count == 0}}"></view>
        <view class='zandata-zan' wx:else>
          <image src='data:image/png;base64,'></image>
          <block wx:for="{{lists[idx].supported.data}}" wx:for-index="v" wx:key="{{v}}">
            <view wx:if="{{lists[idx].supported.count==1}}">{{item.user.nickname}}</view> 
            <view wx:else>{{item.user.nickname}} </view> <view class='zanDou' style='float:left;'>，</view> 
          </block>
        </view>
        <view class='zandata-pinglun'>
        <block wx:for="{{lists[idx].comments}}" wx:key="{{index}}">
          
            <view wx:if="{{item.can_delete}}" bindtap='delComments' data-id="{{item.id}}">
              <text class='zandata-pinglun-nickname'>{{item.user.nickname}}:</text>
              <text class='zandata-pinglun-content'> {{item.content}}</text>
            </view>
            <view wx:else>
              <block wx:if="{{item.user.nickname==undefind}}">
                 <text class='zandata-pinglun-nickname'></text>
              </block>
              <block wx:else>
                <text class='zandata-pinglun-nickname'>{{item.user.nickname}}:</text>
              </block>
              <text class='zandata-pinglun-content'> {{item.content}}</text>
            </view>
         
        </block>
         </view>
      </view>
    </view>
  </block>
</template>
<!-- loading加载 -->
<template name="isHideLoadMore">
  <view class="weui-loadmore" hidden="{{isHideLoadMore}}">
    <view class="weui-loading"></view>
    <view class="weui-loadmore__tips">正在加载</view>
  </view>
</template>